class Solution {
public:
    bool isRectangleOverlap(vector<int>& rec1, vector<int>& rec2) {
         //left
        if (rec2[2] <= rec1[0] || rec1[2] <= rec2[0])
            return false;
        //right
        if (rec2[0] >= rec1[2] || rec1[0] >= rec2[2])
            return false;
        //up
        if (rec2[1] >= rec1[3] || rec1[1] >= rec2[3])
            return false;
        //dowm
        if (rec2[3] <= rec1[1] || rec1[3] <= rec2[1])
            return false;
        return true;

    }
};